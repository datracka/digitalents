    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "dev:next": "next",
    "prebuild": "npm run test && npm run lint",
    "deploy": "now --target production",
    "lint": "eslint 'components' 'lib' 'pages' --ext .js,.jsx",
    "start:next": "NODE_ENV=production next start -p $PORT",
    "prestart:next": "npm run build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "updatedeps": "ncu -u && npm update",
    "dev": "next",
    "build": "next build",
    "start": "next start -p $PORT",
    "heroku-postbuild": "npm run build"
  },
